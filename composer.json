{
    "name": "chrobane/php-discogs-api",
    "description": "The Discogs API makes it easy for developers to communicate with the Discogs platform",
    "type": "library",
    "keywords": ["discogs", "api"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "^6.0",
        "guzzlehttp/guzzle-services": "^1.0",
        "guzzlehttp/oauth-subscriber": "^0.6",
        "bentools/guzzle-throttle-middleware": "^0.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "authors": [
        {
            "name": "Richard van den Brand",
            "email": "richard@vandenbrand.org"
        },
        {
            "name": "Yasushi Kurobane",
            "email": "kurobane@dubstore.co.jp"
        }
    ],
    "autoload": {
        "psr-0": { "Chrobane\\Discogs": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "ricbra/discogs-bundle": "For integrating Discogs into Symfony2"
    }
}
